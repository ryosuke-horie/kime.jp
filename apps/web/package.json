		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^4.4.1",
		"@vitest/coverage-v8": "^3.1.2",
		"autoprefixer": "^10.4.16",
		"happy-dom": "^17.4.6",
		"postcss": "^8.4.32",
