{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"lib": ["ESNext"],
		"types": ["vite/client"],
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"noEmit": true,
		"noPropertyAccessFromIndexSignature": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	},
	"include": ["src/**/*.ts", "src/types/**/*.d.ts", "worker-configuration.d.ts"]
}
